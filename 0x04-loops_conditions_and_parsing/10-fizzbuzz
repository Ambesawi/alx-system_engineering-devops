#!/usr/bin/env bash
# Program that prints FizzBuzz sequence whn divisble to 5 and 3.

for i in {1..100}
do
    if [ $((i % 5)) -eq 0 -a $((i % 3)) -eq 0 ]
    then
	    echo "FizzBuzz"
    elif [ $((i % 5)) -eq 0 ]
    then
	    echo "Buzz"
    elif [ $((i % 3)) -eq 0 ]
    then
	    echo "Fizz"
    else
	    echo "$i"
    fi
done

